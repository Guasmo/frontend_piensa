{
 "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
 "version": 1,
 "newProjectRoot": "projects",
 "projects": {
   "frontend_piensa": {
     "projectType": "application",
     "schematics": {},
     "root": "",
     "sourceRoot": "src",
     "prefix": "app",
     "architect": {
       "build": {
         "builder": "@angular/build:application",
         "options": {
           "browser": "src/main.ts",
           "polyfills": [
             "zone.js"
           ],
           "tsConfig": "tsconfig.app.json",
           "assets": [
             "src/favicon.ico",
             "src/assets"
           ],
           "styles": [
             "src/styles.css"
           ],
           "outputMode": "static",
           "optimization": {
             "scripts": true,
             "styles": {
               "minify": true,
               "inlineCritical": false
             }
           }
         },
         "configurations": {
           "production": {
             "budgets": [
               {
                 "type": "initial",
                 "maximumWarning": "2MB",
                 "maximumError": "5MB"
               },
               {
                 "type": "anyComponentStyle",
                 "maximumWarning": "50kB",
                 "maximumError": "100kB"
               },
               {
                 "type": "bundle",
                 "maximumWarning": "2MB",
                 "maximumError": "5MB"
               }
             ],
             "outputHashing": "all",
             "optimization": {
               "scripts": true,
               "styles": {
                 "minify": true,
                 "inlineCritical": false
               },
               "fonts": true
             },
             "extractLicenses": true,
             "sourceMap": {
               "scripts": false,
               "styles": false,
               "vendor": false
             },
             "namedChunks": false,
             "aot": true,
             "buildOptimizer": false
           },
           "development": {
             "optimization": false,
             "extractLicenses": false,
             "sourceMap": true,
             "namedChunks": true
           },
           "vercel": {
             "budgets": [
               {
                 "type": "initial",
                 "maximumWarning": "5MB",
                 "maximumError": "10MB"
               },
               {
                 "type": "anyComponentStyle",
                 "maximumWarning": "100kB",
                 "maximumError": "200kB"
               }
             ],
             "outputHashing": "all",
             "optimization": {
               "scripts": false,
               "styles": {
                 "minify": false,
                 "inlineCritical": false
               }
             },
             "extractLicenses": false,
             "sourceMap": false,
             "namedChunks": false,
             "aot": true,
             "buildOptimizer": false,
             "vendorChunk": false,
             "commonChunk": false
           }
         },
         "defaultConfiguration": "production"
       },
       "serve": {
         "builder": "@angular/build:dev-server",
         "configurations": {
           "production": {
             "buildTarget": "frontend_piensa:build:production"
           },
           "development": {
             "buildTarget": "frontend_piensa:build:development"
           }
         },
         "defaultConfiguration": "development"
       },
       "extract-i18n": {
         "builder": "@angular/build:extract-i18n"
       },
       "test": {
         "builder": "@angular/build:karma",
         "options": {
           "polyfills": [
             "zone.js",
             "zone.js/testing"
           ],
           "tsConfig": "tsconfig.spec.json",
           "assets": [
             {
               "glob": "**/*",
               "input": "public"
             }
           ],
           "styles": [
             "src/styles.css"
           ]
         }
       }
     }
   }
 },
 "cli": {
   "analytics": "a4a4fe8c-67cc-46f3-ac3e-a69e8ffcb609"
 }
}